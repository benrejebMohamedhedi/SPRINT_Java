<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="703.0" prefWidth="1082.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gestione.shop.AffichageProduitController">
   <children>
      <TableView fx:id="tableproduit" layoutX="36.0" layoutY="46.0" prefHeight="532.0" prefWidth="679.0">
        <columns>
          <TableColumn fx:id="nom" prefWidth="172.0" text="Nom du produit" />
          <TableColumn fx:id="ref" prefWidth="133.0" text="Reference" />
            <TableColumn fx:id="prx" prefWidth="121.0" text="prix" />
            <TableColumn fx:id="qt" prefWidth="87.0" text="quantite" />
        </columns>
      </TableView>
      <TextField fx:id="filterInputs" layoutX="36.0" layoutY="14.0" />
      <Button fx:id="deleteBtn" layoutX="617.0" layoutY="578.0" mnemonicParsing="false" onAction="#handleDeleteButtonClick" prefHeight="31.0" prefWidth="98.0" text="supprimer" />
      <Button fx:id="ajoutergo" layoutX="912.0" layoutY="46.0" mnemonicParsing="false" onAction="#goToAjouter" prefHeight="31.0" prefWidth="139.0" text="ajouter produit" />
      <Button fx:id="modifiergo" layoutX="912.0" layoutY="115.0" mnemonicParsing="false" onAction="#goToModifier" prefHeight="31.0" prefWidth="139.0" text="modifier Produit" />
      <Button fx:id="listt" layoutX="928.0" layoutY="181.0" mnemonicParsing="false" onAction="#gotolist" prefHeight="31.0" prefWidth="115.0" text="list" />
   </children>
</AnchorPane>
